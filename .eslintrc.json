        "quotes": [
            "warn",
            "double"
        ],
        "semi": [
            "warn",
            "always"
        ],
	"eqeqeq": [
	    "error",
	    "always"
	],
	"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
	"max-len": [1, 120, 1, {"ignoreComments": true}],
	"prop-types": [1],
	"no-unused-expressions": "error",
	"no-useless-concat": "error",
	"block-scoped-var": "error",
	"consistent-return": "error"
    }
